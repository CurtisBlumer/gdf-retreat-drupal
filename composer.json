{
    "name": "curtapedia/gdf-retreat-drupal",
    "description": "Composer-based Drupal 8 project for GDF Wellness Retreat",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Curtis Blumer",
            "role": "Developer"
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "^1.5",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.4",
        "drupal/auto_entitylabel": "2.x-dev",
        "drupal/bootstrap": "^3.12",
        "drupal/bootstrap_layouts": "^5.0",
        "drupal/charts": "^3.0@beta",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/console": "^1.8",
        "drupal/core": "~8.5.6",
        "drupal/daterange_compact": "^1.0@dev",
        "drupal/entity_browser_enhanced": "^1.0@RC",
        "drupal/entity_hierarchy": "^2.0@alpha",
        "drupal/geocoder": "^2.0@beta",
        "drupal/geocoder_ajax_prepopulate": "^1.0@beta",
        "drupal/geofield": "^1.0@beta",
        "drupal/geofield_map": "^2.10",
        "drupal/group": "^1.0@RC",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/leaflet": "^1.0@beta",
        "drupal/leaflet_markercluster": "1.x-dev",
        "drupal/media_responsive_thumbnail": "^1.0",
        "drupal/migrate_plus": "^2.0",
        "drupal/migrate_source_csv": "^2.2",
        "drupal/migrate_tools": "^2.0",
        "drupal/name": "^1.0@RC",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.2",
        "drupal/personas": "^1.0@beta",
        "drupal/profile": "^1.0@RC",
        "drupal/radix_layouts": "^4.0",
        "drupal/redhen": "dev-1.x",
        "drupal/redirect": "^1.2",
        "drupal/require_on_publish": "^1.0@beta",
        "drupal/reverse_entity_reference": "dev-2.x",
        "drupal/simple_timeline": "^1.1",
        "drush/drush": "~9.3.0",
        "vlucas/phpdotenv": "^2.5",
        "webflo/drupal-finder": "^1.1",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "apcu-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "enable-patching": true,
        "patches-file": "composer.patches.json",
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes",
                ".htaccess": "../.htaccess"
            }
        }
    }
}
